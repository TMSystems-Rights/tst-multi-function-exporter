{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"version": "1.0.0",
	"description": "__MSG_extDescription__",
	"default_locale": "ja",
	"applications": {
		"gecko": {
			"id": "tst-tab-exporter@tm-systems.jp"
		}
	},
	"permissions": [
		"tabs",
		"downloads",
		"bookmarks",
		"storage"
	],
	"background": {
		"scripts": [
			"common/common.js",
			"background/background.js"
		]
	},
	"browser_action": {
		"default_popup": "popup/popup.html",
		"default_icon": "icon/icon.png",
		"default_title": "__MSG_extName__"
	},
	"web_accessible_resources": [
		"viewer/viewer.html",
		"viewer/viewer.js",
		"viewer/viewer.css",
		"viewer/svg/020_TST/defaultFavicon.svg",
		"viewer/svg/020_TST/extensions.svg",
		"viewer/svg/020_TST/lockwise.svg"
	]
}
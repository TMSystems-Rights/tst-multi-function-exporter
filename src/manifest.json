{
	"manifest_version": 3,
	"name": "__MSG_extName__",
	"version": "2.0.1",
	"description": "__MSG_extDescription__",
	"author": "TMSystems",
	"homepage_url": "https://github.com/TMSystems-Rights/tst-multi-function-exporter",
	"default_locale": "ja",
	"browser_specific_settings": {
		"gecko": {
			"id": "{3414d66d-8a1a-4201-87c7-4741ee9a0556}"
		}
	},
	"permissions": [
		"tabs",
		"downloads",
		"bookmarks",
		"storage"
	],
	"background": {
		"scripts": [
			"common/common.js",
			"background/background.js"
		]
	},
	"action": {
		"default_popup": "popup/popup.html",
		"default_icon": "icon/icon-48.png",
		"default_title": "__MSG_extName__"
	},
	"icons": {
		"48": "icon/icon-48.png",
		"96": "icon/icon-96.png"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"viewer/viewer.html",
				"viewer/viewer.js",
				"viewer/viewer.css",
				"viewer/svg/*.svg"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}